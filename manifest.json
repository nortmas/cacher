{
  "manifest_version": "2",
  "api_version": "1",
  "name": "Cacher",
  "description": "Search in cacher snippets",
  "developer_name": "Dmitry Antonenko",
  "icon": "images/cacher.png",
  "options": {
    "query_debounce": 0.1
  },
  "preferences": [
    {
      "id": "cacher",
      "type": "keyword",
      "name": "Cacher",
      "description": "Find your cacher snippets",
      "default_value": "cc"
    },
    {
      "id": "api_key",
      "type": "input",
      "name": "API Key",
      "description": "Located under Applications menu on your Cacher home page"
    },
    {
      "id": "api_token",
      "type": "input",
      "name": "API Token",
      "description": "Located under Applications menu on your Cacher home page"
    }
  ]
}
